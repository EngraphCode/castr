{
    "name": "openapi-zod-client",
    "version": "1.18.3",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/astahmer/openapi-zod-client.git"
    },
    "author": "Alexandre Stahmer <alexandre.stahmer@gmail.com>",
    "main": "dist/openapi-zod-client.cjs",
    "module": "dist/openapi-zod-client.js",
    "types": "dist/openapi-zod-client.d.ts",
    "exports": {
        ".": {
            "types": "./dist/openapi-zod-client.d.ts",
            "import": "./dist/openapi-zod-client.js",
            "require": "./dist/openapi-zod-client.cjs"
        },
        "./cli": {
            "types": "./dist/cli.d.ts",
            "import": "./dist/cli.js",
            "require": "./dist/cli.cjs"
        }
    },
    "homepage": "https://github.com/astahmer/openapi-zod-client",
    "bin": {
        "openapi-zod-client": "./bin.cjs"
    },
    "scripts": {
        "clean": "rm -rf dist",
        "build": "tsup",
        "dev": "tsup --watch",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "type-check": "tsc --noEmit --project tsconfig.lint.json",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "license": "ISC",
    "dependencies": {
        "@apidevtools/swagger-parser": "^12.1.0",
        "@zodios/core": "^10.9.6",
        "commander": "^14.0.1",
        "handlebars": "^4.7.8",
        "lodash-es": "^4.17.21",
        "openapi-types": "^12.1.3",
        "openapi3-ts": "^4.5.0",
        "prettier": "^3.6.2",
        "tanu": "^0.2.0",
        "ts-pattern": "^5.8.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@eslint/js": "^9.38.0",
        "@types/degit": "^2.8.3",
        "@types/js-yaml": "^4.0.5",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^24.9.1",
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "degit": "^2.8.4",
        "eslint": "^9.38.0",
        "eslint-formatter-pretty": "^7.0.0",
        "eslint-plugin-sonarjs": "^3.0.5",
        "fast-glob": "^3.3.3",
        "tsup": "^8.5.0",
        "type-fest": "^5.1.0",
        "typescript": "^5",
        "typescript-eslint": "^8.46.2",
        "vitest": "^4.0.3"
    },
    "files": [
        "bin.cjs",
        "src",
        "dist",
        "cli",
        "README.md"
    ],
    "keywords": [
        "openapi",
        "swagger",
        "zod",
        "client",
        "zodios",
        "generator",
        "typescript"
    ],
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    }
}
