{
  "name": "openapi-zod-client-monorepo",
  "private": true,
  "version": "1.4.18",
  "repository": {
    "type": "git",
    "url": "https://github.com/astahmer/openapi-zod-client.git"
  },
  "author": "Alexandre Stahmer <alexandre.stahmer@gmail.com>",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --parallel",
    "type-check": "turbo type-check",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "test": "turbo test",
    "test:watch": "turbo test -- --watch",
    "test:snapshot": "turbo test:snapshot",
    "character": "turbo character",
    "test:all": "pnpm test && pnpm character && pnpm test:snapshot",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "clean": "turbo clean",
    "qg": "pnpm build && pnpm format && pnpm type-check && pnpm lint && pnpm test:all",
    "check": "pnpm clean && pnpm install && pnpm qg",
    "fetch:examples": "turbo fetch:examples"
  },
  "license": "ISC",
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.11"
    }
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "ajv-draft-04": "^1.0.0",
    "ajv-formats": "^3.0.1",
    "pnpm": "^10.19.0",
    "prettier": "^3.6.2",
    "tsx": "^4",
    "turbo": "^2.5.8",
    "typescript": "^5"
  }
}
