{
    "name": "openapi-zod-client-monorepo",
    "private": true,
    "version": "1.4.18",
    "repository": {
        "type": "git",
        "url": "https://github.com/astahmer/openapi-zod-client.git"
    },
    "author": "Alexandre Stahmer <alexandre.stahmer@gmail.com>",
    "engines": {
        "node": ">=20.0.0"
    },
    "packageManager": "pnpm@10.19.0",
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev --parallel",
        "type-check": "turbo type-check",
        "lint": "turbo lint",
        "lint:fix": "turbo lint -- --fix",
        "test": "turbo test",
        "test:watch": "turbo test -- --watch",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
        "verify": "turbo format:check type-check lint test",
        "clean": "turbo clean && rm -rf node_modules",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "gen:samples": "rm -rf ./samples && tsx ./lib/samples-generator.ts",
        "release": "pnpm changeset publish"
    },
    "license": "ISC",
    "pnpm": {
        "overrides": {
            "esbuild": "0.25.11"
        }
    },
    "dependencies": {
        "@babel/core": "^7.19.6",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-typescript": "^7.18.6"
    },
    "devDependencies": {
        "@changesets/changelog-github": "^0.4.8",
        "@changesets/cli": "^2.26.0",
        "@preconstruct/cli": "^2.8.12",
        "conventional-changelog-cli": "^2.2.2",
        "pnpm": "^10.19.0",
        "prettier": "^3.6.2",
        "tsx": "^4",
        "turbo": "^2.5.8",
        "typescript": "^5"
    }
}
