{
    "name": "openapi-zod-client-monorepo",
    "private": true,
    "version": "1.4.18",
    "repository": {
        "type": "git",
        "url": "https://github.com/astahmer/openapi-zod-client.git"
    },
    "preconstruct": {
        "packages": [
            "lib"
        ]
    },
    "author": "Alexandre Stahmer <alexandre.stahmer@gmail.com>",
    "engines": {
        "node": ">=20.0.0"
    },
    "packageManager": "pnpm@10.19.0",
    "scripts": {
        "dev": "preconstruct watch",
        "build": "preconstruct build",
        "test": "pnpm --filter=openapi-zod-client test:ci",
        "type-check": "tsc --noEmit --project tsconfig.lint.json",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "postinstall": "preconstruct dev",
        "gen:samples": "rm -rf ./samples && tsx ./lib/samples-generator.ts",
        "release": "pnpm changeset publish"
    },
    "license": "ISC",
    "pnpm": {
        "overrides": {
            "esbuild": "0.25.11"
        }
    },
    "dependencies": {
        "@babel/core": "^7.19.6",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-typescript": "^7.18.6"
    },
    "devDependencies": {
        "@changesets/changelog-github": "^0.4.8",
        "@changesets/cli": "^2.26.0",
        "@preconstruct/cli": "^2.8.12",
        "conventional-changelog-cli": "^2.2.2",
        "pnpm": "^10.19.0",
        "prettier": "^3.6.2",
        "tsx": "^4",
        "typescript": "^5"
    }
}
